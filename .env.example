# CropCare 360 Environment Configuration

# ML Diagnosis API (optional - uses fallback if not provided)
ML_ENDPOINT=https://api.example.com/diagnose
ML_KEY=your_ml_api_key_here

# Payment Integration (Razorpay test mode)
RAZORPAY_KEY_ID=rzp_test_1234567890
RAZORPAY_KEY_SECRET=test_secret_key_here

# Database Configuration
DB_PATH=./db/cropcare.db

# Server Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=your_super_secret_jwt_key_here

# Feature Flags
ENABLE_ML_DIAGNOSIS=true
ENABLE_EXPERT_QUEUE=true
ENABLE_OFFLINE_SYNC=true

# Rate Limiting
MAX_DIAGNOSES_PER_MINUTE=5
MAX_FILE_SIZE_MB=10

# Admin Configuration
ADMIN_EMAIL=admin@cropcare360.com
ADMIN_PASSWORD=admin123

# Logging
LOG_LEVEL=info
DEBUG=false